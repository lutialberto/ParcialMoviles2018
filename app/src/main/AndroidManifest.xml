<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.betom.parcialmoviles2018">

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controllers.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controllers.Exercise1Activity" />
        <activity android:name=".controllers.Exercise2Activity" />
        <activity android:name=".controllers.MultiplicationActivity" />
        <activity android:name=".controllers.Exercise3Activity" />
        <activity android:name=".controllers.Exercise4Activity" />

        <service
            android:name=".services.HelloService"
            android:enabled="true"
            android:exported="true"
            android:permission="string"/>
        <service
            android:name=".services.MyIntentService"
            android:exported="true"
            android:permission="string"/>
    </application>

</manifest>